resource "aws_vpc" "mock_vpc" {
  cidr_block = "10.98.188.0/22"
}

module "s3" {
  source = "./modules/s3"
  aws_account_number = var.aws_account_number
  aws_region = var.aws_region
  environment = var.environment
}

module "sqs" {
  source = "./modules/sqs"
  app_file_storage = module.s3.primary_app_file_storage
  aws_account_number = var.aws_account_number
  aws_region = var.aws_region
  environment = var.environment
}

module "secrets_manager" {
  source = "./modules/secrets_manager"
  aws_account_number = var.aws_account_number
  environment = var.environment
}

resource "aws_kms_key" "global_kms_key" {}
resource "aws_kms_key" "visa_itf_block_key" {}
resource "aws_kms_alias" "visa_itf_block_key" {
  target_key_id = aws_kms_key.visa_itf_block_key.id
  name = "alias/${var.environment}-${var.aws_region}-visa-itf-block-key"
}

module "param-store" {
  depends_on = [aws_kms_key.global_kms_key]
  source = "./modules/param-store"
  environment = var.environment
  pgp_private_key = "segments:5"
  pgp_private_key_part1 = ""
  pgp_private_key_part2 = ""
  pgp_private_key_part3 = ""
  pgp_private_key_part4 = ""
  pgp_private_key_part5 = ""
  kms_key_id = aws_kms_key.global_kms_key.id
}

module "sns-topics" {
  source = "./modules/sns"
  aws_region = var.aws_region
  environment = var.environment
}